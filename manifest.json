{
  "name": "Todoist: Styles",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "adds styles to todoist",
  "homepage_url": "https://apricot13.org",
  "host_permissions": [
    "https://todoist.com/app/*",
    "https://todoist.com/app/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://todoist.com/app/*", "https://todoist.com/app/*"],
      "js": ["inject.js"],
      "css": ["inject.css"]
    }
  ]
}
